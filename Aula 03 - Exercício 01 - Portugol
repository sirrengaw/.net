programa
{

	funcao inicio()
	{
		inteiro n1,n2,n3,max

		escreva("Digite o primeiro numero: ")
		leia(n1)
		max = n1
		escreva("Digite o segundo numero: ")
		leia(n2)
		se (n2 > max){
			max = n2
		}
		escreva("Digite o terceiro numero: ")
		leia(n3)
		se (n3 > max){
			max = n3
		}
		escreva("O numero maior digitado Ã©: ",max)
	}
}
